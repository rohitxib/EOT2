    override func viewDidLoad() {
        super.viewDidLoad()
        
       // APP_Delegate.currentVC = "client"
        lblClient.text = LanguageKey.empty_client_list
       // NotiyCenterClass.registerNotiHandlerNotifier(vc: self, selector: #selector(self.chatfNotiHandle(_:)))
