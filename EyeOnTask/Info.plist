	<array>
		<string>Colaborate-Regular.ttf</string>
		<string>Colaborate-Bold.ttf</string>
		<string>Colaborate-Light.ttf</string>
		<string>Colaborate-Medium.ttf</string>
		<string>Arimo-BoldItalic.ttf</string>
		<string>Arimo-Italic.ttf</string>
		<string>Arimo-Regular.ttf</string>
		<string>Arimo-Bold.ttf</string>
	</array>
